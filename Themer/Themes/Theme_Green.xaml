    <Color x:Key="ForegroundSelected">#B6C2CF</Color>
    <Color x:Key="ForegroundInactive">#B6C2CF</Color>
    <Color x:Key="ForegroundDisabled">#55B6C2CF</Color>
    <Color x:Key="ForegroundMouseOver">#B6C2CF</Color>
    <Color x:Key="ForegroundPressed">#B6C2CF</Color>
