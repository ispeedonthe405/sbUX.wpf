<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Spectrum0">#CECECE</Color>
    <Color x:Key="Spectrum1">#BEBEBE</Color>
    <Color x:Key="Spectrum2">#AEAEAE</Color>
    <Color x:Key="Spectrum3">#9E9E9E</Color>
    <Color x:Key="Spectrum4">#8E8E8E</Color>
    <Color x:Key="Spectrum5">#7E7E7E</Color>
    <Color x:Key="Spectrum6">#6E6E6E</Color>
    <Color x:Key="Spectrum7">#5E5E5E</Color>
    <Color x:Key="Spectrum8">#4E4E4E</Color>
    <Color x:Key="Spectrum9">#3E3E3E</Color>
